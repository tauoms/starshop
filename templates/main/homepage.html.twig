{% extends 'base.html.twig' %}

{% block title %}
	{{parent()}}
	New title
{% endblock %}

{% block body %}
	<div>
		Browse through
		{{ numberOfStarships }}
		starships.

		{% if numberOfStarships > 400 %}
			<p>That's a lot of starships!</p>
		{% endif %}
	</div>

	<table>
		{% for key,value in myShip %}
			<tr>
				<td>{{key}}</td>
				<td>{{value}}</td>
			</tr>
		{% endfor %}
	</table>

{% endblock %}
